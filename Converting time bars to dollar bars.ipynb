   "id": "e8498027",
   "metadata": {},
   "outputs": [],
   "source": [
    "def fetch_crypto_data(symbol,timeframe,adjuster,data_size):\n",
    "    \n",
    "    \"\"\"\n",
    "    input\n",
    "    - symbol\n",
    "    - timeframe\n",
    "    - adjuster: adjusts timestamp to match the amount of 200 data. \n",
    "    - data_size(int, >2): data_size*200 = df.shape[0]\n",
    "        \n",
    "    output\n",
    "    - df: contains df.shape[0] data points in descending order \n",
    "    \n",
    "    \"\"\"\n",
    "    \n",
    "    ohlcv_1 = pd.Series(bybit.fetchOHLCV(symbol=symbol,timeframe=timeframe, limit=200))\n",
    "    df = [ohlcv_1,]\n",
    "    \n",
    "    for i in range(2,data_size+1):\n",
    "        ohlcv = pd.Series(bybit.fetchOHLCV(symbol='BTCUSDT',timeframe=timeframe, \n",
    "                                 since=df[i-2][0][0]-adjuster*60*1000))\n",
    "        df.append(ohlcv)\n",
    "    df = df[::-1] # 最新データが底に来るように\n",
    "    df = pd.concat(df)\n",
    "    df = list(df)\n",
    "    df.insert(0,['timestamp','open','high','low','close','volume'])\n",
    "    df = pd.DataFrame(df,columns=df[0])\n",
    "    df = df.drop(df.index[0]).reset_index(drop=True)\n",
    "    \n",
    "    df['timestamp'] = df['timestamp'].apply(lambda x: datetime.fromtimestamp(x/1000))\n",
    "    df['timestamp'] = pd.to_datetime(df['timestamp'],format='&Y%m%d %H:%M')\n",
    "    df.set_index('timestamp',inplace=True)\n",
    "    \n",
    "    df.loc[pd.to_datetime('2022-03-18 06:00:00')<=df.index,'maker fee']= 0.0001\n",
    "    df.loc[pd.to_datetime('2022-03-18 06:00:00')>df.index,'maker fee']= -0.00025\n",
    "    \n",
    "    return df"
   ]
  },
